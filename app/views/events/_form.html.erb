<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :eventname %>
    <%= f.input :eventlocation %>
    <%= f.input :eventdate %>
    <%= f.input :eventnotes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
