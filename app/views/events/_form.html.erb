<%= simple_form_for @event, html: {multipart: true} do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :eventname %>
    <%= f.input :eventlocation %>
    <%= f.input :eventdate %>
    <%= f.input :eventnotes %>
    <%= f.input :deadline %>
  </div>

  <div class="form-group">
    <%= f.label :spreadsheet %>
    <%= f.file_field :image, class: "form-control" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
