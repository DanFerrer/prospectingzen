<%= render 'layouts/nav_sheets' %>
<div class="content-events">
    <h1 style="margin:25px 0 60px 0">My Events <span style="float:right"><%= link_to 'New Event', new_event_path, class: "home-buttons" %><span></h1>

      <p>Your assistant will use this information to find prospectects who are attending this event.</p>

<% @events.each do |event| %>
  <p><%= link_to "#{event.eventname}, #{event.eventlocation}", event %></strong><span class="small"><%= link_to 'Edit', edit_event_path(event) %>
        <%= link_to 'Delete', event, method: :delete, data: { confirm: 'Are you sure?' } %></span></p><hr style="margin-top:-20px">

<% end %>



<br>


</div>